{
  "name": "@beep/api",
  "version": "1.0.0",
  "description": "GraphQL API for the Beep App",
  "main": "build/index.js",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "concurrently --raw \"tsc -w --preserveWatchOutput\" \"nodemon build/index.js\"",
    "db:create": "mikro-orm schema:create --run",
    "db:update": "mikro-orm schema:update --run",
    "db:drop": "mikro-orm schema:drop --run",
    "db:migrate": "mikro-orm migration:up",
    "db:migration:create": "mikro-orm migration:create"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.nussman.us/beep-app/Beep.git"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./build/mikro-orm.config.js"
    ]
  },
  "author": "Banks Nussman",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "^8.5.1",
    "@mikro-orm/core": "5.5.3",
    "@mikro-orm/postgresql": "5.5.3",
    "@sentry/integrations": "^7.17.3",
    "@sentry/node": "^7.17.3",
    "@sentry/tracing": "^7.17.3",
    "apollo-server-core": "^3.10.3",
    "apollo-server-express": "^3.10.3",
    "aws-sdk": "^2.1249.0",
    "bcrypt": "^5.1.0",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "got": "^11.8.3",
    "graphql": "^15.6.1",
    "graphql-fields-to-relations": "^1.0.5",
    "graphql-redis-subscriptions": "^2.5.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^12.0.0",
    "graphql-ws": "^5.11.2",
    "ioredis": "^5.1.0",
    "js-sha256": "^0.9.0",
    "nodemailer": "^6.8.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "uuid": "^9.0.0",
    "wkx": "^0.5.0",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "5.5.3",
    "@mikro-orm/migrations": "5.5.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.14",
    "@types/graphql-upload": "^8.0.5",
    "@types/node": "^18.7.23",
    "@types/nodemailer": "^6.4.6",
    "@types/uuid": "^8.3.3",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "concurrently": "^7.5.0",
    "eslint": "^8.27.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "volta": {
    "node": "18.10.0"
  }
}
